	<aura:attribute name="contracts" type="List" />
	<aura:attribute name="contractsList" type="List" />
    <aura:attribute name="gasContract" type="Object" />
    <aura:attribute name="count" type="String" default="0" />
    <aura:attribute name="contractMessage" type="String" default="契約情報取得中..."/>
    <aura:attribute name="lableName" type="String" default="グループ" />
    <aura:handler name="init" action="{!c.init}" value="{!this}" />
    <aura:attribute name="popFlg" type="boolean" default="false" description="ポップアップ判定用フラグ" />
    <aura:attribute name="errorFlg" type="boolean" default="false"  />
    <!-- add by yuan -->
    <aura:attribute name="elContract" type="Object" />
    
    <aura:attribute name="triplePopFlg" type="boolean" default="false" description="ポップアップ判定用フラグ" />
    <aura:attribute name="elPopFlg" type="boolean" default="false" description="ポップアップ判定用フラグ" />
    <aura:attribute name="elePopFlg" type="boolean" default="false" description="ポップアップ判定用フラグ" />
